varnishtest "Director cleanup fails on vcl.discard"

server s1 {
	rxreq
	txresp
} -start

varnish v1 -vcl+backend {
	director foo random {
		{ .backend = s1; .weight = 1; }
		{ .backend = { .host = "${s1_addr}"; .port = "${s1_port}";} .weight =1; }
		{ .backend = { .host = "${s1_addr}"; .port = "${s1_port}";} .weight =1; }
	}
	sub vcl_recv {
		set req.backend = foo;
	}
} -start


varnish v1 -vcl+backend { }

varnish v1 -cliok "vcl.list"
varnish v1 -cliok "vcl.discard vcl1"

client c1 {
	txreq
	rxresp
} -run
